# declaracion de variables x y y
x= int(input("digite el valor para la variable x: "))
y= int(input("digite el valor para la variable y: "))
suma=x+y
resta=x-y
multiplicacion=x*y
division=x/y
modulo=x%y

print("El resultado de la suma es :", suma)

print(f"la resta de {x} y {y} es {resta}")
print("el resultado de la multiplicacion es :",multiplicacion)
print("el resultado de la division es :",division)
print("el resultado del modulo es :",modulo)
